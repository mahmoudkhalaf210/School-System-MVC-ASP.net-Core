@using Microsoft.AspNetCore.Identity
@model List<IdentityRole>
@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<a asp-action="new" asp-controller="Role" class="btn btn-success">Add Role</a>

<table class="table table-bordered w-25 text-center">
    <tr>
        <th>Role</th>
    </tr>
    
        @foreach (var role in Model)
        {   
            <tr>
                <td>@role.Name</td>
            </tr>
        }
    
</table>